						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
					</object>
					<object class="sizeritem">
						<object class="wxStaticText" name="ID_STATICTEXT3" variable="StaticText3" member="yes">
							<label>-- Language --</label>
						</object>
						<flag>wxALL|wxALIGN_LEFT</flag>
						<border>5</border>
						<option>1</option>
					</object>
				</object>
				<flag>wxTOP|wxLEFT|wxRIGHT|wxEXPAND</flag>
				<border>5</border>
			</object>
			<object class="sizeritem">
				<object class="wxBoxSizer" variable="BoxSizer10" member="no">
					<object class="sizeritem">
						<object class="wxRadioBox" name="ID_RADIOBOX5" variable="RadioBox5" member="yes">
							<label></label>
							<content>
								<item> system  </item>
								<item> it  </item>
								<item> en </item>
							</content>
							<default>-1</default>
							<validator>wxGenericValidator(&amp;language)</validator>
							<style>wxRA_SPECIFY_ROWS|wxBORDER_NONE</style>
						</object>
						<flag>wxBOTTOM|wxLEFT|wxRIGHT|wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
